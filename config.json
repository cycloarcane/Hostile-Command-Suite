    },
    "duckduckgo": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/duckduckgo_osint.py"]
    },
    "google": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/google_osint.py"],
      "transport": "stdio"
    },
    "shodan": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/shodan_osint.py"],
      "transport": "stdio"
    },
    "metadata": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/metadata_osint.py"],
      "transport": "stdio"
    },
    "domain": {
      "command": ".venv/bin/python",
