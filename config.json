      "transport": "stdio"
    },
    "geolocation": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/geolocation_osint.py"],
      "transport": "stdio"
    },
    "breach": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/breach_osint.py"],
      "transport": "stdio"
    },
    "crypto": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/crypto_osint.py"],
      "transport": "stdio"
    },
    "certificate": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/certificate_osint.py"],
      "transport": "stdio"
    },
    "social": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/social_osint.py"],
      "transport": "stdio"
    },
    "nmap": {
      "command": ".venv/bin/python",
