    "email": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/email_osint.py"]
    },
    "username": {
      "command": ".venv/bin/python",
      "args": ["-u", "OSINT/username_osint.py"]
    },
    "phone": {
      "command": ".venv/bin/python",
